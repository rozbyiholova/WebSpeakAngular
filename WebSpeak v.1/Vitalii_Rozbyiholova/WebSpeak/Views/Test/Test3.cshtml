@model List<DTO>
@using DAL.Models;
@{
    ViewData["Title"] = "Test3";
    var Data = Html.Raw(Json.Serialize(Model.ToArray()));
}
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/js/Tests.js"></script>
<link rel="stylesheet" type="text/css" href="~/css/TestStyle.css" />

<div class="test container">
    <div class="test_images row">

    </div>
    <div class="test_word"></div>
    <button class="confirm btn-success">Check</button>
</div>

<div class="test_result" style="display: none;">
    <button class="save-result btn-success">Finish</button>
</div>


<script type="text/javascript">
    $(document).ready(() => {
        InitNewTest(@Data, 1, 4, 0);
        NextTest();
    })
</script>